        },
        {
            "name": "Author Name",
            "code": "author_name",
            "description": "Name of the author",
            "type": "text_replace"
        },
        {
            "name": "Author Email",
            "code": "author_email",
            "description": "Email of the author",
            "type": "text_replace"
        }
    ]
